    <fields>
        <fullName>BD_Number_of_Program_Years_or_Term__c</fullName>
        <description>Number of years from start date to end date plus any residual years included in the deal.</description>
        <externalId>false</externalId>
        <formula>IF( AND(BD_LOC_Years__c =0, OR(BD_LOC_Months__c=0,BD_LOC_Months__c&gt;0)),
    IF( ISPICKVAL(Account_Protective_Provision__c,&apos;Yes&apos;) ,
        BD_LOC_Years__c+1+VALUE(TEXT( Account_Protective_Provision_Years__c )),
        BD_LOC_Years__c+1
    ),
    IF(AND(BD_LOC_Years__c&gt;0, BD_LOC_Months__c=0),
        IF( ISPICKVAL(Account_Protective_Provision__c,&apos;Yes&apos;) ,
            BD_LOC_Years__c+VALUE(TEXT( Account_Protective_Provision_Years__c )),
            BD_LOC_Years__c
        ),
        IF(AND(BD_LOC_Years__c&gt;0, BD_LOC_Months__c&gt;0),
            IF( ISPICKVAL(Account_Protective_Provision__c,&apos;Yes&apos;) ,
                BD_LOC_Years__c+1+VALUE(TEXT( Account_Protective_Provision_Years__c )),
                BD_LOC_Years__c+1
            ),
            0
        )
    )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Number of years from start date to end date plus any residual years included in the deal.</inlineHelpText>
        <label>Number of Program Years (or Term)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>BD_Payback__c</fullName>
        <externalId>false</externalId>
        <formula>(T1_Payback_Years__c*12 ) / ( (BD_Period_in_months__c)+ IF( ISPICKVAL(Account_Protective_Provision__c, &apos;Yes&apos;) , VALUE( TEXT( Account_Protective_Provision_Years__c ))*12, 0) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Payback %</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>BD_Period_in_months__c</fullName>
        <externalId>false</externalId>
        <formula>IF ( ISPICKVAL(Deal_Type__c,&quot;MasterCard Processing&quot;),
12-1+MONTH(End_Date__c)+12*(YEAR(End_Date__c)-YEAR( IPS_Start_Date__c )-1),
12-MONTH(Start_Date__c)+MONTH(End_Date__c)+12*(YEAR(End_Date__c)-YEAR(Start_Date__c)-1))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Period in months</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
