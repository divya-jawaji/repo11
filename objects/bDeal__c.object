    <validationRules>
        <fullName>Ariba_deal_name_can_not_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Deal_Name__c),
  OR(
    Text(Deal_Status__c)==&apos;Active&apos;,
    Text(Deal_Status__c)== &apos;Dead&apos;,
    Text(Deal_Status__c)==&apos;Rejected&apos;,
    Text(Deal_Status__c)==&apos;Expired&apos;,
    (AND(Text(Deal_Status__c)== &apos;Business Case Approved Internally&apos; ,Text( Ariba_Contract_Request_Status__c)==&apos;Initiated the Ariba Contract Request&apos;)))
)</errorConditionFormula>
        <errorDisplayField>Deal_Name__c</errorDisplayField>
        <errorMessage>The Deal Name cannot be changed after an ARIBA Contract Request has been made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BD_Admin_Profile_Validate_Status_Change</fullName>
        <active>false</active>
        <errorConditionFormula>IF( 
AND( $Profile.Name = &apos;Finance BD Admin&apos; , 
ISCHANGED( Deal_Status__c), 
NOT(ISPICKVAL(Deal_Status__c, &apos;Expired&apos;))) , 
true,
false
)</errorConditionFormula>
        <errorDisplayField>Deal_Status__c</errorDisplayField>
        <errorMessage>You don&apos;t have permission to change Deal Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BD_No_Create_Deal</fullName>
        <active>true</active>
        <description>A deal can not be created if the stage of the opportunity associated is equals to &quot;Closed won&quot; or &quot;Closed Not Won&quot; or &quot;Closed deferred&quot;</description>
        <errorConditionFormula>(ISPICKVAL(Opportunity__r.StageName, &quot;Closed Not Won&quot;)  || 
ISPICKVAL(Opportunity__r.StageName, &quot;Closed Won&quot;)  || 
ISPICKVAL(Opportunity__r.StageName, &quot;Closed deferred&quot;)  ) &amp;&amp;  ISNEW()</errorConditionFormula>
        <errorDisplayField>Nr_of_Returns__c</errorDisplayField>
        <errorMessage>A deal can not be created if the stage of the opportunity associated is &quot;Closed Won&quot; or &quot;Not Won&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Account_Provision_Years</fullName>
        <active>false</active>
        <description>The Account Provisions Years must be less than LOC Years</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false,
IF( ISPICKVAL(Account_Protective_Provision__c, &quot;YES&quot;),
     IF( BD_LOC_Years__c &lt;= VALUE(TEXT(Account_Protective_Provision_Years__c)),
        true,
        false
    ),
    false
)

)</errorConditionFormula>
        <errorDisplayField>Account_Protective_Provision_Years__c</errorDisplayField>
        <errorMessage>The Account Provisions Years must be less than LOC Years</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Master_Deal_when_deal_is_active</fullName>
        <active>true</active>
        <description>Can&apos;t change Master deal when Oppt is won and Deal is active</description>
        <errorConditionFormula>if (contains( $Profile.Name, &quot;System Admin&quot;) , False ,
And (contains (text(Opportunity__r.StageName) , &quot;Closed Won&quot;),  ISCHANGED( Master_Deal__c ) ) )</errorConditionFormula>
        <errorMessage>Can&apos;t change master Deal when Oppt is close won (the master deal is already active)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Clawback_explanation_both_no_and_yes</fullName>
        <active>true</active>
        <description>Values cannot be entered in both the &quot;If Yes, Describe Clawback Calculation&quot; and &quot;If No, Explain (Clawback)&quot; field</description>
        <errorConditionFormula>If_no_please_explain_clawback__c &lt;&gt; &apos;&apos; &amp;&amp;  If_yes_describe_calculation_for_clawbac__c &lt;&gt; &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Will_agreement_include_Clawback_Provisi__c</errorDisplayField>
        <errorMessage>Values cannot be entered in both the &quot;If Yes, Describe Clawback Calculation&quot; and &quot;If No, Explain (Clawback)&quot; fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Request_Workspace_Id_Format</fullName>
        <active>true</active>
        <description>As a part of ARIBA-195</description>
        <errorConditionFormula>IF(NOT(ISBLANK(Ariba_Contract_Request_Workspace_Id__c)),IF(NOT(OR(BEGINS(Ariba_Contract_Request_Workspace_Id__c, &apos;CW&apos;),BEGINS(Ariba_Contract_Request_Workspace_Id__c, &apos;CRW&apos;), BEGINS(Ariba_Contract_Request_Workspace_Id__c, &apos;LCW&apos;) )) , true, false),False)</errorConditionFormula>
        <errorMessage>The Contract ID entered does not have the proper format. Please use the Contract ID from Ariba i.e. CWxxxxxxx OR CRWxxxxxxx OR LCWxxxxxx.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Same_Opportunity_Account</fullName>
        <active>true</active>
        <description>The Customer must the same than the Account of the Opportunity</description>
        <errorConditionFormula>AND(
NOT CONTAINS ($Profile.Name, &apos;System Admin&apos;),
Customer__c !=  Opportunity__r.AccountId)</errorConditionFormula>
        <errorDisplayField>Customer__c</errorDisplayField>
        <errorMessage>The Customer must the same than the Account of the Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Support_only_For_Merchant_Agree</fullName>
        <active>true</active>
        <description>The Following Customer Support type is only allowed for the Merchant Agreement deals:
- Exclusivity for Merchant Acceptance</description>
        <errorConditionFormula>NOT( ISPICKVAL(Deal_Type__c, &quot;Merchant Agreement&quot; )) &amp;&amp; INCLUDES(Type_of_Customer_Support__c, &quot;Exclusivity for Merchant Acceptance&quot;)</errorConditionFormula>
        <errorDisplayField>Type_of_Customer_Support__c</errorDisplayField>
        <errorMessage>Exclusivity for Merchant Acceptance as a customer support type should only be allowed for Merchant Agreement deals</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_can_not_be_changed</fullName>
        <active>true</active>
        <description>Customer can not be changed by a user other the Admin.</description>
        <errorConditionFormula>ISCHANGED( Customer__c )  &amp;&amp; NOT CONTAINS($Profile.Name, &apos;System Admin&apos;)</errorConditionFormula>
        <errorMessage>You cannot change the customer on an existing deal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DFO_Edit_No_Override_User</fullName>
        <active>true</active>
        <description>When a record is in Override Mode, only the User who entered the Override Mode, can edit the record.</description>
        <errorConditionFormula>AND(
Override_Mode__c = true, 
NOT(ISCHANGED(Potential_Financial_Model__c)), /*A user can change the attachments */
PRIORVALUE(Override_Mode__c)= true, /* to be able to enter the override mode */
NOT(LastModifiedById = $User.Id))</errorConditionFormula>
        <errorMessage>When a record is in Override Mode, only the User who locked the record can edit the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DFO_Edit_Start_and_End_Date</fullName>
        <active>true</active>
        <description>When a user updates the start and end date in Override Mode causing the LOC Yr and/or LOC Months values to change</description>
        <errorConditionFormula>AND(
Override_Mode__c = true,
OR(
PRIORVALUE(BD_LOC_Months__c) &lt;&gt; BD_LOC_Months__c,
PRIORVALUE(BD_LOC_Years__c)  &lt;&gt; BD_LOC_Years__c)
)</errorConditionFormula>
        <errorMessage>The dates entered will change the term of the deal. Please Recall the deal and then change the dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deal_Covering_Countries_cannot_be_empty</fullName>
        <active>false</active>
        <description>Deal Covering Countries cannot be empty</description>
        <errorConditionFormula>ISBLANK(Deal_covering_countries__c )</errorConditionFormula>
        <errorMessage>Deal Covering Countries cannot be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deal_Status_verification</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISPICKVAL( Cobrand_Flag_PL__c, &apos;&apos; )) &amp;&amp;
(ISPICKVAL(Deal_Status__c, &apos;Business Case Development&apos;) ||
ISPICKVAL(Deal_Status__c, &apos;Business Case Returned&apos;)) &amp;&amp;(ISPICKVAL(PRIORVALUE(Cobrand_Flag_PL__c), &apos;No&apos;) &amp;&amp;   ISPICKVAL(Cobrand_Flag_PL__c, &apos;Yes&apos;) || ISPICKVAL(PRIORVALUE(Cobrand_Flag_PL__c), &apos;Yes&apos;) &amp;&amp;   ISPICKVAL(Cobrand_Flag_PL__c, &apos;No&apos;))</errorConditionFormula>
        <errorDisplayField>Cobrand_Flag_PL__c</errorDisplayField>
        <errorMessage>This field can be edited only if the deal status is &apos;Business Case Development&apos; or &apos;Business Case Returned&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_Deal_not_possible_After_Submitted</fullName>
        <active>true</active>
        <description>It&apos;s not possible to delete a deal once it has been submitted.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Deal_Status__c, &quot;Business Case Submitted for Approval&quot;), 
Deal_Deletion__c = true)</errorConditionFormula>
        <errorMessage>It&apos;s not possible to delete a deal with the status Business Case Submitted for Approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_Deal_only_for_admin</fullName>
        <active>true</active>
        <description>Only Admins are allowed to delete a deal. (To check the Deletion Deal checkbox, since this is needed to delete a deal)</description>
        <errorConditionFormula>AND( 
NOT CONTAINS($Profile.Name, &apos;System Admin&apos;), 
Deal_Deletion__c = true)</errorConditionFormula>
        <errorMessage>Only System Administrators are allowed to delete a deal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IMK_Fast_Track_Eligible_Is_No_Explain</fullName>
        <active>true</active>
        <description>When a user places a deal into Override Mode, if the new IMK Fast Track Eligible field is No then the If No explain field is required</description>
        <errorConditionFormula>AND( PRIORVALUE(Override_Mode__c) = true &amp;&amp; Override_Mode__c = true, ISPICKVAL( IMK_FTE__c , &quot;No&quot;), ISPICKVAL( If_No_Select_Why_Deal_Isn_t_Eligible__c,&apos;&apos; ) )</errorConditionFormula>
        <errorMessage>If No is selected for IMK Fast Track Eligible field then the If No explain field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Is_this_early_renewal_required</fullName>
        <active>true</active>
        <description>the &quot;Is this early renewal &quot; field is required on save if the opportunity TYPE field is one fo the following &apos;New business (with existing customer)&apos; or &apos;Renewal Business&apos;</description>
        <errorConditionFormula>AND(
    ISPICKVAL(OpportunityType__c, &quot;Renewal Business&quot;), 
    ISBLANK( TEXT(Is_this_early_renewal__c) )
)</errorConditionFormula>
        <errorDisplayField>Is_this_early_renewal__c</errorDisplayField>
        <errorMessage>the &quot;Is this early renewal &quot; field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LOC_Sup_1_Month</fullName>
        <active>true</active>
        <description>- The EndDate must be greater than the Start Date
- The Start Date and the End Date cannot have the same month and year.
- minimum 1 month</description>
        <errorConditionFormula>OR(AND( MONTH( Start_Date__c )  =  MONTH( End_Date__c )  , YEAR( Start_Date__c )  =  YEAR( End_Date__c ) ),End_Date__c  &lt;  Start_Date__c )</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>The End Date must be greater than the Start Date and the dates can not be in the same month of the same year.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Data_Source_Other_Value</fullName>
        <active>true</active>
        <description>check that Other value is filed when for &quot;Other Data Source&quot;</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, AND (  INCLUDES(Data_Source__c, &quot;Other&quot;)  , ISBLANK( Data_Source_Other__c  ) ) )</errorConditionFormula>
        <errorDisplayField>Data_Source_Other__c</errorDisplayField>
        <errorMessage>Other value must be filed when for &quot;Other&quot; is selected in  Data Source</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Describe_Clawback_calculation</fullName>
        <active>true</active>
        <description>If Will agreement incl Clawback Provision equals yes then a description of the calculation is required</description>
        <errorConditionFormula>ISPICKVAL(Will_agreement_include_Clawback_Provisi__c , &quot;Yes&quot;) &amp;&amp;  If_yes_describe_calculation_for_clawbac__c = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>If_yes_describe_calculation_for_clawbac__c</errorDisplayField>
        <errorMessage>Please describe clawback calculation!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_If_no_clawback_please_explain</fullName>
        <active>true</active>
        <description>Explain why clawback equals no</description>
        <errorConditionFormula>ISPICKVAL(Will_agreement_include_Clawback_Provisi__c , &quot;NO&quot;) &amp;&amp;  If_no_please_explain_clawback__c = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>If_no_please_explain_clawback__c</errorDisplayField>
        <errorMessage>Please explain if agreement does not include Clawback!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Is_this_early_renewal</fullName>
        <active>true</active>
        <description>the &quot;Is this early renewal &quot; field must be empty on save if the opportunity TYPE field is &apos;New Customer&apos;</description>
        <errorConditionFormula>IF(
    contains( $Profile.Name , &quot;System Admin&quot;), false, 
    AND(
        ISPICKVAL(OpportunityType__c ,&quot;New Business&quot;), 
        NOT(ISBLANK( TEXT(Is_this_early_renewal__c) ) )
    )  
)</errorConditionFormula>
        <errorDisplayField>Is_this_early_renewal__c</errorDisplayField>
        <errorMessage>the &quot;Is this early renewal &quot; field must be empty (-- None --) for New Customer or New Business.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Reason_for_additional_mastercard</fullName>
        <active>false</active>
        <description>Please explain Additional commitments</description>
        <errorConditionFormula>NOT( ISBLANK( Add_l_MCard_Commitments__c ) ) &amp;&amp;  Please_explain_Additional_commitments__c = &apos;&apos; &amp;&amp;
NOT(ISPICKVAL(Deal_Type__c , &apos;Other Customer Investment&apos;))&amp;&amp;
NOT(ISPICKVAL(Deal_Type__c , &apos;Merchant Agreement&apos;))&amp;&amp;
NOT(ISPICKVAL(Deal_Type__c , &apos;MasterCard Processing&apos;))</errorConditionFormula>
        <errorDisplayField>Please_explain_Additional_commitments__c</errorDisplayField>
        <errorMessage>Please explain Additional commitments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Residual_Year_Revenue</fullName>
        <active>true</active>
        <description>If Does pr deal have residual yr revenue  is set to yes you can not add values in No Residual Year Revenue is entered</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, 


ISPICKVAL( Does_pr_deal_have_residual_yr_revenue__c , &quot;No&quot;) &amp;&amp; No_Residual_Year_Revenue_Explain__c = &apos;&apos;    )</errorConditionFormula>
        <errorDisplayField>No_Residual_Year_Revenue_Explain__c</errorDisplayField>
        <errorMessage>Please explain why prior/existing deal has no residual years of revenue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Residual_Years_If_No</fullName>
        <active>true</active>
        <description>If no, Explain (Residual Revenue) cannot be filled when &quot;Yes&quot; is selected in Residual Years on Existing Deal</description>
        <errorConditionFormula>AND(
ISPICKVAL( Does_pr_deal_have_residual_yr_revenue__c ,&quot;Yes&quot;), 
 No_Residual_Year_Revenue_Explain__c  &lt;&gt; &quot;&quot;)</errorConditionFormula>
        <errorMessage>If no, Explain (Residual Revenue) cannot be filled when &quot;Yes&quot; is selected in Residual Years on Existing Deal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Support_Prepayment_Amount</fullName>
        <active>true</active>
        <description>check that Support Prepayment Amount is filled when Missing Support Prepayment question is YES</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, AND (   CONTAINS(TEXT(Customer_Support_Prepayment__c), &quot;Yes&quot;)   ,    ISBLANK(Customer_Support_Prepayment_Amount__c ) )  )</errorConditionFormula>
        <errorDisplayField>Customer_Support_Prepayment_Amount__c</errorDisplayField>
        <errorMessage>Customer Support Prepayment Amount cannot be blank if Customer Support Prepayment is YES</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Support_Prepayment_Desc</fullName>
        <active>true</active>
        <description>check that Support Prepayment description is filed when Missing Support Prepayment question is YES</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, 
 (

AND ( CONTAINS(TEXT(Customer_Support_Prepayment__c), &quot;Yes&quot;) , ISBLANK( Customer_Support_Prepayment_Description__c ) ) ) )</errorConditionFormula>
        <errorDisplayField>Customer_Support_Prepayment_Description__c</errorDisplayField>
        <errorMessage>Customer Support Prepayment Description cannot be blank if Customer Support Prepayment is YES</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_TypeofCust_support_Other</fullName>
        <active>true</active>
        <description>Type of customer support &quot;Other&quot; must be blank if Type of Customer Support selection  does not contain &quot;Other&quot;</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, 

AND ( NOT(INCLUDES( Type_of_Customer_Support__c , &quot;Other&quot;)) , LEN( Type_of_Customer_Support_Other__c  )  &gt; 0)  )</errorConditionFormula>
        <errorDisplayField>Type_of_Customer_Support_Other__c</errorDisplayField>
        <errorMessage>This field must be blank if Type of Customer Support selection  does not contain &quot;Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_TypeofCust_support_Other_Value</fullName>
        <active>true</active>
        <description>Type of customer support &quot;Other&quot; cannot be blank if Type of Customer Support selection  is &quot;Other&quot;</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, 

AND ( INCLUDES( Type_of_Customer_Support__c , &quot;Other&quot;) , LEN( TRIM(Type_of_Customer_Support_Other__c ) ) = 0 )   )</errorConditionFormula>
        <errorDisplayField>Type_of_Customer_Support_Other__c</errorDisplayField>
        <errorMessage>This field cannot be blank if Type of Customer Support selection  is &quot;Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_residual_years</fullName>
        <active>false</active>
        <description>Add Number of residual years when &quot;Does pr deal have residual yr revenue &quot; equals yes</description>
        <errorConditionFormula>IF(contains( $Profile.Name , &quot;System Admin&quot;), false, ISPICKVAL(Does_pr_deal_have_residual_yr_revenue__c, &apos;Yes&apos;) &amp;&amp;  ISPICKVAL(Number_of_residual_years__c, &apos;&apos;)   )</errorConditionFormula>
        <errorDisplayField>Number_of_residual_years__c</errorDisplayField>
        <errorMessage>Number of residual years is required!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multi_Country_in_Deal_Covering_Countries</fullName>
        <active>true</active>
        <description>No Multi-Country in Deal Covering Countries is allowed.</description>
        <errorConditionFormula>AND( 
NOT CONTAINS($Profile.Name, &apos;System Admin&apos;), 
CONTAINS (Deal_covering_countries__c, &quot;Multi&quot;))</errorConditionFormula>
        <errorMessage>You cannot select a Multi Country as a Deal Covering Country</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Type_uneditable</fullName>
        <active>true</active>
        <description>The opportunity type is not to be changed at deal level.</description>
        <errorConditionFormula>AND(
NOT(CONTAINS( $Profile.Name, &quot;System Admin&quot;)), 
TEXT(OpportunityType__c)  &lt;&gt;   TEXT(Opportunity__r.Type ))</errorConditionFormula>
        <errorMessage>You cannot change the opportunity type on this deal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_can_not_be_changed</fullName>
        <active>true</active>
        <description>Opportunity can not be changed by a user other then admin.</description>
        <errorConditionFormula>ISCHANGED( Opportunity__c )  &amp;&amp; NOT CONTAINS($Profile.Name, &apos;Admin&apos;)</errorConditionFormula>
        <errorMessage>You cannot change the opportunity on an existing deal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_is_required</fullName>
        <active>false</active>
        <description>A deal must be linked to an Opportunity</description>
        <errorConditionFormula>Opportunity__c =&quot;&quot;</errorConditionFormula>
        <errorMessage>A deal must be linked to an Opportunity</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>