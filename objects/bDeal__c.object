        <fullName>Total_Calendar_Months__c</fullName>
        <externalId>false</externalId>
        <formula>(IF(ISPICKVAL(Account_Protective_Provision__c, &apos;Yes&apos;) ,
  CASE(YEAR(End_Date__c) - YEAR( Start_Date__c ),
    0, MONTH(End_Date__c) - MONTH( Start_Date__c )+1 + 12* VALUE(TEXT( Account_Protective_Provision_Years__c )) ,
    1, 13-MONTH(Start_Date__c ) + MONTH(End_Date__c) +12* VALUE(TEXT(Account_Protective_Provision_Years__c )),
    (YEAR(End_Date__c) - YEAR( Start_Date__c )-1+  VALUE(TEXT(Account_Protective_Provision_Years__c )))*12 + MONTH(End_Date__c) + 13- MONTH( Start_Date__c )
  ),
    CASE(YEAR(End_Date__c) - YEAR( Start_Date__c ),
    0, MONTH(End_Date__c) - MONTH( Start_Date__c )+1 ,
    1, 13-MONTH(Start_Date__c ) + MONTH(End_Date__c),
    (YEAR(End_Date__c) - YEAR( Start_Date__c )-1)*12 + MONTH(End_Date__c) + 13- MONTH( Start_Date__c )
   )
)
)
-( BD_Contract_Duration__c - BD_Period_in_months__c )</formula>
